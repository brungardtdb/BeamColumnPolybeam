<Window x:Class="BeamColumnPolybeam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeamColumnPolybeam"
        mc:Ignorable="d"
        Title="MainWindow" Height="195.546" Width="279.738" Background="#FF703131">
    <Grid Margin="0,0,2,0">
        <Button x:Name="btnBeam" Content="Beam" HorizontalAlignment="Left" Margin="29,27,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="btnColumn" Content="Column" HorizontalAlignment="Left" Margin="29,67,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button x:Name="btnPolyBeam" Content="PolyBeam" HorizontalAlignment="Left" Margin="29,110,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <TextBox x:Name="txtProfile" HorizontalAlignment="Left" Height="23" Margin="121,27,0,0" TextWrapping="Wrap" Text="Profile" VerticalAlignment="Top" Width="131" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="txtColumnHeight" HorizontalAlignment="Left" Height="23" Margin="121,64,0,0" TextWrapping="Wrap" Text="Column Height (inches)" VerticalAlignment="Top" Width="131" TextChanged="txtColumnHeight_TextChanged"/>

    </Grid>
</Window>
